{
    "name": "nkamuo/barcode-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony bundle for barcode generation and processing.",
    "keywords": ["symfony", "barcode", "qr-code", "ean", "gtin", "data-matrix", "bundle"],
    "license": "MIT",
    "authors": [
        {
            "name": "Callistus Nkamuo",
            "email": "callistusnkamuo@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "picqer/php-barcode-generator": "^3.2",
        "endroid/qr-code": "^6.0",
        "symfony/dependency-injection": "^7",
        "symfony/http-kernel": "^7",
        "lamoda/gs1-barcode-parser": "^1.1",
        "doctrine/dbal": "*"
    },
    "autoload": {
        "psr-4": {
            "Nkamuo\\Barcode\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        },
        "symfony": {
            "recipe": {
                "repo": "github.com/symfony/recipes-contrib",
                "branch": "main"
            }
        }
    }
}
